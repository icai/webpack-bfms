{
  "presets": [
    'react',
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [ "react-hot-loader/babel", "transform-react-jsx", "transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-react-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
